@using System.Data
@using FP.Manager
@{
    DataTable dt = new DataTable();
    dt = Model;
}
<style>
    .box-title {
        background: #4CAF50;
        color: #fff;
        padding: 8px 10px;
        border-radius: 10px 10px 0 0;
        margin-top: 10px;
        word-wrap: break-word;
    }

        .box-title div {
            padding: 5px 0
        }
    /*
        .box-title span {
            float: right;
        }*/

    .table-box {
        margin-bottom: 15px;
        background: #DEFFDF;
        padding: 12px 0 12px 12px;
        /*border: 1px solid #000;*/
        border-spacing: 10px;
        border-collapse: separate;
        word-wrap: break-word;
    }

    table {
        border-collapse: separate;
        border-spacing: 0px;
        /* Apply cell spacing */
    }

    table,
    th,
    td {
        border: 1px solid #666;
        text-align: center;
    }

        table th,
        table td {
            padding: 5px;
            /* Apply cell padding */
        }
</style>
@if (dt.Rows.Count > 0)
{
    foreach (DataRow dr in dt.Rows)
    {
        <div class="row">
            <div class="col-md-12">
                <p class="text-center"> <strong>परिवार नियोजन अभिसरण कार्यक्रम</strong></p>
                <p class="text-center"><strong>सी. एम. के द्वारा भरा जाने वाला योग्य दम्पत्ति सूची (लाइन लिस्ट फॉर्मेट सह फॉलो अप) प्रपत्र</strong>  </p>
                <p class="text-center"><strong>प्रपत्र-1</strong> </p>
            </div>
            <div class="box-title">
                <div class="rounded h-100 p-4 row g-4">
                    <div class="col-sm-12 col-xl-4">
                        <div class="mb-3">
                            सीएम का नाम :<span>@dr["DistrictName"]</span>
                        </div>
                        <div class="mb-3">
                            मोबाईल नंबर:<span>@dr["DistrictName"]</span>
                        </div>
                        <div class="mb-3">
                            ग्राम संगठन का नाम        :<span>@dr["DistrictName"]</span>
                        </div>
                        <div class="mb-3">
                            कुल समूहों की संख्या         :<span>@dr["DistrictName"]</span>
                        </div>
                    </div>
                    <div class="col-sm-12 col-xl-3">
                        <div class="mb-3">
                            गाँव      :<span>@dr["DistrictName"]</span>
                        </div>
                        <div class="mb-3">
                            पँचायत         :<span>@dr["DistrictName"]</span>
                        </div>
                        <div class="mb-3">
                            प्रखंड        :<span>@dr["DistrictName"]</span>
                        </div>
                        <div class="mb-3">
                            जिला         :<span>@dr["DistrictName"]</span>
                        </div>
                    </div>
                    <div class="col-sm-12 col-xl-5">
                        <div class="mb-3">
                            सी.एन. आर.पी का नाम      :<span>@dr["DistrictName"]</span>
                        </div>
                        <div class="mb-3">
                            सी. एन. आर. पी.का
                            मोबाइल नंबर          :<span>@dr["DistrictName"]</span>
                        </div>
                        <div class="mb-3">
                            स्वास्थ्य उपकेन्द्र
                            का नाम स्थान        :<span>@dr["DistrictName"]</span>
                        </div>
                        <div class="mb-3">
                            माह &amp; वर्ष          :<span>@dr["DistrictName"]</span>
                        </div>
                    </div>
                </div>
            </div>
            <div class="table-box">
                <table style="width:95%;">
                    <thead>
                        <tr>
                            <th>क्र. सं.</th>
                            <th>समूह का नाम</th>
                            <th>लाभार्थी का नाम</th>
                            <th>लाभार्थी की उम्र (वर्ष मे)</th>
                            <th>लाभार्थी के पति का नाम </th>
                            <th>लाभार्थी की शादी की तिथि (वर्ष मे)</th>
                        </tr>
                    </thead>
                    <tbody>
                    </tbody>
                </table>
            </div>
        </div>
    }
}
else
{
    <h4 class="text-success">@Enums.GetEnumDescription(Enums.eReturnReg.RecordNotFound)</h4>
}
