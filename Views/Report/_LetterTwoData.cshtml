@using System.Data
@using FP.Manager
@{
    DataTable dt = new DataTable();
    dt = Model;
}
<style>
    .box-title {
        background: #4CAF50;
        color: #fff;
        padding: 15px 20px;
        border-radius: 10px 10px 0 0;
        margin-top: 10px;
    }

        .box-title div {
            padding: 5px 0
        }

        .box-title span {
            float: right;
        }

    .table-box {
        margin-bottom: 15px;
        background: #DEFFDF;
        padding: 12px 0 12px 12px;
        /*border: 1px solid #000;*/
        border-spacing: 10px;
        border-collapse: separate;
        word-wrap: break-word;
    }
    table {
        border-collapse: separate;
        border-spacing: 0px;
        /* Apply cell spacing */
    }

    table,
    th,
    td {
        border: 1px solid #666;
        text-align:center;
    }

        table th,
        table td {
            padding: 5px;
            /* Apply cell padding */
        }
</style>
@if (dt.Rows.Count > 0)
{
    foreach (DataRow dr in dt.Rows)
    {
        var colSum1 = (!string.IsNullOrEmpty(dr["Noofnochild"].ToString()) && !string.IsNullOrEmpty(dr["Noofnochild1"].ToString()))
            ? (Convert.ToInt32(dr["Noofnochild"]) + Convert.ToInt32(dr["Noofnochild1"])) : 0;
        var colSum2 = (!string.IsNullOrEmpty(dr["Noofonechild"].ToString()) && !string.IsNullOrEmpty(dr["Noofonechild1"].ToString()))
           ? (Convert.ToInt32(dr["Noofonechild"]) + Convert.ToInt32(dr["Noofonechild1"])) : 0;
        var colSum3 = (!string.IsNullOrEmpty(dr["Nooftwochild"].ToString()) && !string.IsNullOrEmpty(dr["Nooftwochild1"].ToString()))
           ? (Convert.ToInt32(dr["Nooftwochild"]) + Convert.ToInt32(dr["Nooftwochild1"])) : 0;
        var colSum4 = (!string.IsNullOrEmpty(dr["Noofmorechild"].ToString()) && !string.IsNullOrEmpty(dr["Noofmorechild1"].ToString()))
           ? (Convert.ToInt32(dr["Noofmorechild"]) + Convert.ToInt32(dr["Noofmorechild1"])) : 0;

        var colSum5 = (!string.IsNullOrEmpty(dr["Noofrolevideo"].ToString()) && !string.IsNullOrEmpty(dr["Noofrolevideo1"].ToString()))
           ? (Convert.ToInt32(dr["Noofrolevideo"]) + Convert.ToInt32(dr["Noofrolevideo1"])) : 0;
        var colSum6 = (!string.IsNullOrEmpty(dr["Noofmoduleroleout"].ToString()) && !string.IsNullOrEmpty(dr["Noofmoduleroleout1"].ToString()))
           ? (Convert.ToInt32(dr["Noofmoduleroleout"]) + Convert.ToInt32(dr["Noofmoduleroleout1"])) : 0;

        var colSum7 = (!string.IsNullOrEmpty(dr["Nooffollowup"].ToString()) && !string.IsNullOrEmpty(dr["Nooffollowup1"].ToString()))
           ? (Convert.ToInt32(dr["Nooffollowup"]) + Convert.ToInt32(dr["Nooffollowup1"])) : 0;
        var colSum8 = (!string.IsNullOrEmpty(dr["NooffwupPm"].ToString()) && !string.IsNullOrEmpty(dr["NooffwupPm1"].ToString()))
           ? (Convert.ToInt32(dr["NooffwupPm"]) + Convert.ToInt32(dr["NooffwupPm1"])) : 0;
        var colSum9 = (!string.IsNullOrEmpty(dr["NooffwupTm"].ToString()) && !string.IsNullOrEmpty(dr["NooffwupTm1"].ToString()))
           ? (Convert.ToInt32(dr["NooffwupTm"]) + Convert.ToInt32(dr["NooffwupTm1"])) : 0;
        var colSum10 = (!string.IsNullOrEmpty(dr["NoofOm"].ToString()) && !string.IsNullOrEmpty(dr["NoofOm1"].ToString()))
           ? (Convert.ToInt32(dr["NoofOm"]) + Convert.ToInt32(dr["NoofOm1"])) : 0;
        var colSum11 = (!string.IsNullOrEmpty(dr["NoofPm"].ToString()) && !string.IsNullOrEmpty(dr["NoofPm1"].ToString()))
           ? (Convert.ToInt32(dr["NoofPm"]) + Convert.ToInt32(dr["NoofPm1"])) : 0;
        var colSum12 = (!string.IsNullOrEmpty(dr["NoofTm"].ToString()) && !string.IsNullOrEmpty(dr["NoofTm1"].ToString()))
           ? (Convert.ToInt32(dr["NoofTm"]) + Convert.ToInt32(dr["NoofTm1"])) : 0;

        <hr />
        <div class="row">
            <div class="col-md-12">
                <p class="text-center"> <strong>परिवार नियोजन अभिसरण कार्यक्रम</strong></p>
                <p class="text-center"><strong>सी. एम. का मासिक प्रतिवेदन सह प्रोत्तसाहन राशि मांग प्रपत्र</strong>  </p>
                <p class="text-center"><strong>प्रपत्र-2</strong> </p>
            </div>
            <div class="box-title">
                <div class="col-md-12">
                    <div class="col-sm-3">ज़िला         :<span>@dr["DistrictName"]</span></div>
                    <div class="col-sm-3">ब्लॉक         :<span>@dr["Block"]</span></div>
                    <div class="col-sm-3">पंचायत        :<span>@dr["Panchayat"]</span></div>
                    <div class="col-sm-3">ग्राम संगठन    :<span>@dr["Village_Organization"]</span></div>
                    <div class="col-sm-3">गाँव          :<span>@dr["VillageName"]</span></div>
                    <div class="col-sm-3">रिपोर्ट सबमिट   :<span>@dr["EmpName"]</span></div>
                    <div class="col-sm-3">माह &amp; वर्ष :<span>@dr["ReportingMonthYear"]</span></div>
                    @*इनके द्वारा रिपोर्ट की गई*@

                </div>
            </div>
            <div class="table-box">
                <table style="width:95%;">
                    <thead>
                        <tr><th>क्र.सं.</th><th>गतिविधियों <br /> के नाम</th><th>पिछले माह में</th><th>वर्तमान <br /> माह में</th><th>कुल  <br />(संख्या में)</th></tr>
                    </thead>
                    <tbody>
                        <tr><td rowspan="4" style="word-wrap: break-word;">समूह परिवार के कुल लक्षित लाभार्थियों का सूची निर्माण</td><td>(1)नव दंपति/वैसी महिलाओं की संख्या जिन्हें अभी बच्चा नहीं है</td><td>@dr["Noofnochild"]</td><td>@dr["Noofnochild1"]</td><td>@colSum1</td></tr>
                        <tr><td>(2)वैसी महिलाओं की संख्या जीने अभी एक बच्चा है</td><td>@dr["Noofonechild"]</td><td>@dr["Noofonechild1"]</td><td>@colSum2</td></tr>
                        <tr><td>(3)वैसी महिलाओं की संख्या जिन्हें दो बच्चे हैं</td><td>@dr["Nooftwochild"]</td><td>@dr["Nooftwochild1"]</td><td>@colSum3</td></tr>
                        <tr><td>(4) वैसी महिलाओं की संख्या जिन्हें दो से अधिक बच्चे हैं</td><td>@dr["Noofmorechild"]</td><td>@dr["Noofmorechild1"]</td><td>@colSum4</td></tr>

                        <tr><td rowspan="2">समूह बैठक पणजी के अनुसार (मॉड्यूल रोलआउट/ वीडियो प्रसारण)</td><td>(1)कितने समूह में मॉड्यूल रोल आउट के माध्यम से जागरूक किया गया</td><td>@dr["Noofrolevideo"]</td><td>@dr["Noofrolevideo1"]</td><td>@colSum5</td></tr>
                        <tr><td>(2)कितने समूह में वीडियो के माध्यम से जागरूक किया गया</td><td>@dr["Noofmoduleroleout"]</td><td>@dr["Noofmoduleroleout"]</td><td>@colSum6</td></tr>

                        <tr><td rowspan="6">लाभार्थी से फॉलो-अप</td><td>(1)कितने लाभार्थियों के यहां फॉलो अप किया गया</td><td>@dr["Nooffollowup"]</td><td>@dr["Nooffollowup1"]</td><td>@colSum7</td></tr>
                        <tr><td>(2)कितने लाभार्थी अस्थाई साधन का उपयोग कर रहे हैं</td><td>@dr["NooffwupPm"]</td><td>@dr["NooffwupPm1"]</td><td>@colSum8</td></tr>
                        <tr><td>(3)कितने लाभार्थी स्थाई साधन का उपयोग कर रहे हैं</td><td>@dr["NooffwupTm"]</td><td>@dr["NooffwupTm1"]</td><td>@colSum9</td></tr>
                        <tr><td>(4)कितने लाभार्थी परिवार नियोजन के कोई भी साधन उपयोग नहीं कर रहे हैं</td><td>@dr["NoofOm"]</td><td>@dr["NoofOm1"]</td><td>@colSum10</td></tr>
                        <tr><td>(5)कितने लाभार्थी अस्थाई साधन का उपयोग करना चाहते हैं</td><td>@dr["NoofPm"]</td><td>@dr["NoofPm1"]</td><td>@colSum11</td></tr>
                        <tr><td>(6)कितने लाभार्थी अस्थाई साधन का उपयोग करना चाहते हैं</td><td>@dr["NoofTm"]</td><td>@dr["NoofTm1"]</td><td>@colSum12</td></tr>

                    </tbody>
                </table>
            </div>
        </div>
    }
}