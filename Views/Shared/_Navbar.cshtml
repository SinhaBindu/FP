@using FP.Manager
@{
    var u = MvcApplication.CUser;
    var IsViewer = false;
}
<!-- Sidebar Start -->
<style>
    .fp-logo {
        width: 70px;
        margin-right: 10px;
    }
</style>

<div class="sidebar pe-4 pb-3">
    <a href="/" class="navbar-brand mx-4 mb-3">
        <h6 class="text-primary">
