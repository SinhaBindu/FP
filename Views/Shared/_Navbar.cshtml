<script>
    setTimeout(function () {
        currentUrl = $(location).attr("href");
        $('.nav-item > .dropdown-menu a').each(function (i, item) {
            if (item.href == currentUrl) {
                $(item).addClass('active');
                $(item).parent('div').prev('a').addClass('show').attr('aria-expanded', true);
                $(item).parent('div').addClass('show');
            }
        });

    }, 100);
</script>