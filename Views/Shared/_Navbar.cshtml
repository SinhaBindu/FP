                        <a href="#">
                            New <span class="arrow"></span>
                        </a>
                    </li>
                </ul>
                <ul class="sub-menu collapse" id="fame">
                    <li>New New 1</li>
                    <li>New New 2</li>
                    <li>New New 2</li>
                </ul>
                <li data-toggle="collapse" data-target="#follow-up" class="collapsed">
                    <a href="#">
                        <i class="fa  fa-arrow-up me-2"></i>Follow up <span class="arrow"></span>
                    </a>
                </li>
                <ul class="sub-menu collapse" id="follow-up">
                    <li><a href="~/Beneficiary/BFYFollow">Follow up</a></li>
                    <li><a href="~/Beneficiary/FollowupList">Follow List</a></li>
                </ul>
                <li> <a href="/Achievement/AddAchievePlan"> <i class="fa fa-fa fa-users me-2"></i> Achievement </a> </li>
