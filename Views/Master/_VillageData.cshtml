@using System.Data
@{ 
	DataTable dt = new DataTable();
	dt = Model;
}


<table class="table table-bordered table-striped" id="tbl">
	<thead>
		<tr>
			<th>Srno.</th>
			<th>District</th>
			<th>Block</th>
			<th>Panchayat</th>
			<th>Village</th>
		</tr>
	</thead>
	<tbody>
		@{ 
			if (dt.Rows.Count>0)
			{

				int srno = 1;
				foreach (DataRow dr in dt.Rows)
				{
					<tr>
						<td>@(srno++)</td>
						<td>@dr["DistrictName"]</td>
						<td>@dr["Block"]</td>
						<td>@dr["Panchayat"]</td>
						<td>@dr["Village_Organization"]</td>
					    <td>
                            <a href="~/Master/VillMaster?id=@dr["Void_pk"]" class="btn btn-primary">Edit <i class="fa fa-edit"></i></a>
                        </td>
					</tr>
				}
			}
			else
			{
				<tr>
					<td colspan="10" class="text-center"><strong class="text-danger">Record Not Found !!</strong></td>
				</tr>
			}
		}
	</tbody>

</table>